
#Aliases
alias g="firefox http://www.google.com"
alias yt="firefox http://www.youtube.com"
alias wf="firefox https://www.wolframalpha.com"
alias ts="teamspeak"
alias dsc="discord"
alias lis='ls -X -1'
alias gitcnp='git commit -m "autocomm" && git push'
alias wakeLap='sudo etherwake -i eth0 insert_mac'
#comando per cd + ls
cl()
{
    cd $@
    lis
}
#comando per upload github
gitup()
{
git add $@
gitcnp
}

